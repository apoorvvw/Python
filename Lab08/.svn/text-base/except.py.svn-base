#!/usr/local/bin/python3.4

__author__ = 'ee364e10'


if __name__ == "__main__":
    str = input ("Enter characters seperated by white space: ")
    value = 0
    values = str.split()
    for i in values:
        try:
            value += float(i)
        except(ValueError):
            pass

    # print(values)
    print("The sum is:" , value)
